  "definitions": {
    "publication.schema.json": {
      "$id": "publication.schema.json",
      "title": "Web Publication Manifest",
      "type": "object",
      "properties": {
        "@context": {
          "default": [
            "https://schema.org",
            "https://www.w3.org/ns/pub-context"
          ],
          "type": "array",
          "items": {
            "type": "string",
            "format": "uri"
          },
          "allOf": [
            {
              "contains": {
                "const": "https://schema.org"
              }
            },
            {
              "contains": {
                "const": "https://www.w3.org/ns/wp-context"
              }
            }
          ],
          "uniqueItems": true
        },
        "url": {
          "type": "string",
          "format": "uri"
        },
        "id": {
          "type": "string",
          "format": "uri"
        },
        "type": {
          "default": [
            "Book"
          ],
          "type": "array",
          "items": {
            "enum": [
              "3DModel",
              "ArchiveComponent",
              "Article",
              "Atlas",
              "Blog",
              "Book",
              "Chapter",
              "Claim",
              "Clip",
              "Collection",
              "ComicStory",
              "Comment",
              "Conversation",
              "Course",
              "CreativeWorkSeason",
              "CreativeWorkSeries",
              "DataCatalog",
              "Dataset",
              "DefinedTermSet",
              "Diet",
              "DigitalDocument",
              "Drawing",
              "EducationalOccupationalCredential",
              "Episode",
              "ExercisePlan",
              "Game",
              "HowTo",
              "HowToDirection",
              "HowToSection",
              "HowToStep",
              "HowToTip",
              "Legislation",
              "Manuscript",
              "Map",
              "MediaObject",
              "Menu",
              "MenuSection",
              "Message",
              "Movie",
              "MusicComposition",
              "MusicPlaylist",
              "MusicRecording",
              "Painting",
              "Photograph",
              "Play",
              "Poster",
              "PublicationIssue",
              "PublicationVolume",
              "Question",
              "Quotation",
              "Review",
              "Sculpture",
              "SheetMusic",
              "ShortStory",
              "SoftwareApplication",
              "SoftwareSourceCode",
              "TVSeason",
              "TVSeries",
              "Thesis",
              "VisualArtwork",
              "WebContent",
              "WebPage",
              "WebPageElement",
              "WebSite"
            ],
            "type": "string"
          },
          "uniqueItems": true
        },
        "name": {
          "title": "Title",
          "$ref": "#/definitions/localizable.schema.json",
          "minItems": 1,
          "default": [
            ""
          ]
        },
        "artist": {
          "title": "Artist",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "author": {
          "title": "Author",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "colorist": {
          "title": "Colorist",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "contributor": {
          "title": "Contributor",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "creator": {
          "title": "Creator",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "editor": {
          "title": "Editor",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "illustrator": {
          "title": "Illustrator",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "inker": {
          "title": "Inker",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "letterer": {
          "title": "Letterer",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "penciler": {
          "title": "Penciler",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "publisher": {
          "title": "Publisher",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "readBy": {
          "title": "Read By",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "translator": {
          "title": "Translator",
          "type": "array",
          "items": {
            "$ref": "#/definitions/contributor-object.schema.json"
          }
        },
        "dateModified": {
          "type": "string",
          "anyOf": [
            {
              "title": "Date",
              "format": "date"
            },
            {
              "title": "Date with time",
              "format": "date-time"
            }
          ]
        },
        "datePublished": {
          "type": "string",
          "anyOf": [
            {
              "title": "Date",
              "format": "date"
            },
            {
              "title": "Date with time",
              "format": "date-time"
            }
          ]
        },
        "duration": {
          "type": "string",
          "pattern": "^P(?!$)((\\d+Y)|(\\d+\\.\\d+Y$))?((\\d+M)|(\\d+\\.\\d+M$))?((\\d+W)|(\\d+\\.\\d+W$))?((\\d+D)|(\\d+\\.\\d+D$))?(T(?=\\d)((\\d+H)|(\\d+\\.\\d+H$))?((\\d+M)|(\\d+\\.\\d+M$))?(\\d+(\\.\\d+)?S)?)??$"
        },
        "direction": {
          "type": "string",
          "enum": [
            "rtl",
            "ltr"
          ],
          "default": "ltr"
        },
        "inDirection": {
          "type": "string",
          "default": "auto",
          "enum": [
            "rtl",
            "ltr",
            "auto"
          ]
        },
        "inLanguage": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/language.schema.json"
          },
          "default": [
            "rus"
          ],
          "uniqueItems": true
        },
        "accessMode": {
          "type": "array",
          "items": {
            "enum": [
              "auditory",
              "tactile",
              "textual",
              "visual",
              "colorDependent",
              "chartOnVisual",
              "chemOnVisual",
              "diagramOnVisual",
              "mathOnVisual",
              "musicOnVisual",
              "textOnVisual"
            ],
            "type": "string"
          },
          "default": [
            "textual",
            "visual"
          ],
          "uniqueItems": true
        },
        "accessModeSufficient": {
          "type": "array",
          "items": {
            "enum": [
              "auditory",
              "tactile",
              "textual",
              "visual"
            ],
            "type": "string"
          },
          "default": [
            "textual",
            "visual"
          ],
          "uniqueItems": true
        },
        "accessibilityFeature": {
          "type": "array",
          "items": {
            "enum": [
              "alternativeText",
              "annotations",
              "audioDescription",
              "bookmarks",
              "braille",
              "captions",
              "ChemML",
              "describedMath",
              "displayTransformability",
              "highContrastAudio",
              "highContrastDisplay",
              "index",
              "largePrint",
              "latex",
              "longDescription",
              "MathML",
              "none",
              "printPageNumbers",
              "readingOrder",
              "rubyAnnotations",
              "signLanguage",
              "structuralNavigation",
              "synchronizedAudioText",
              "tableOfContents",
              "taggedPDF",
              "tactileGraphic",
              "tactileObject",
              "timingControl",
              "transcript",
              "ttsMarkup",
              "unlocked"
            ],
            "type": "string"
          },
          "uniqueItems": true
        },
        "accessibilityHazard": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "flashing",
              "noFlashingHazard",
              "motionSimulation",
              "noMotionSimulationHazard",
              "sound",
              "noSoundHazard",
              "unknown",
              "none"
            ]
          },
          "default": [],
          "uniqueItems": true
        },
        "accessibilitySummary": {
          "$ref": "#/definitions/localizable.schema.json"
        },
        "links": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "type": "string",
                "format": "uri-reference"
              },
              {
                "$ref": "#/definitions/link.schema.json"
              }
            ]
          },
          "uniqueItems": true
        },
        "readingOrder": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "type": "string",
                "format": "uri-reference"
              },
              {
                "$ref": "#/definitions/link.schema.json"
              }
            ]
          },
          "uniqueItems": true
        },
        "resources": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "type": "string",
                "format": "uri-reference"
              },
              {
                "$ref": "#/definitions/link.schema.json"
              }
            ]
          },
          "uniqueItems": true
        }
      },
      "required": [
        "@context",
        "url",
        "type"
      ]
    },
    "contributor.schema.json": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "$id": "contributor.schema.json",
      "title": "Contributor",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "#/definitions/contributor-object.schema.json"
      },
      "uniqueItems": true
    },
    "contributor-object.schema.json": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "$id": "contributor-object.schema.json",
      "type": "object",
      "properties": {
        "name": {
          "$ref": "#/definitions/localizable.schema.json"
        },
        "id": {
          "type": "string",
          "format": "uri"
        },
        "type": {
          "type": "string",
          "default": "Person",
          "enum": [
            "Person",
            "Organization"
          ]
        }
      },
      "required": [
        "name"
      ]
    },
    "localizable.schema.json": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "value": {
            "type": "string"
          },
          "language": {
            "$ref": "#/definitions/language.schema.json"
          }
        },
        "uniqueItems": true,
        "required": [
          "value"
        ]
      },
      "localizable-map.schema.json": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "object",
            "patternProperties": {
              "^((?:(en-GB-oed|i-ami|i-bnn|i-default|i-enochian|i-hak|i-klingon|i-lux|i-mingo|i-navajo|i-pwn|i-tao|i-tay|i-tsu|sgn-BE-FR|sgn-BE-NL|sgn-CH-DE)|(art-lojban|cel-gaulish|no-bok|no-nyn|zh-guoyu|zh-hakka|zh-min|zh-min-nan|zh-xiang))|((?:([A-Za-z]{2,3}(-(?:[A-Za-z]{3}(-[A-Za-z]{3}){0,2}))?)|[A-Za-z]{4}|[A-Za-z]{5,8})(-(?:[A-Za-z]{4}))?(-(?:[A-Za-z]{2}|[0-9]{3}))?(-(?:[A-Za-z0-9]{5,8}|[0-9][A-Za-z0-9]{3}))*(-(?:[0-9A-WY-Za-wy-z](-[A-Za-z0-9]{2,8})+))*(-(?:x(-[A-Za-z0-9]{1,8})+))?)|(?:x(-[A-Za-z0-9]{1,8})+))$": {
                "type": "string"
              }
            },
            "additionalProperties": false,
            "minProperties": 1
          }
        ]
      }
    },
