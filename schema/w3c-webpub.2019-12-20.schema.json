    "link.schema.json": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "$id": "https://w3c.github.io/pub-manifest/schema/link.schema.json",
      "title": "Publication Links",
      "type": "object",
      "properties": {
        "type": {
          "anyOf": [
            {
              "type": "string",
              "const": "LinkedResource"
            },
            {
              "type": "array",
              "items": {
                "type": "string"
              },
              "contains": {
                "const": "LinkedResource"
              }
            }
          ]
        },
        "url": {
          "type": "string",
          "format": "iri-reference"
        },
        "encodingFormat": {
          "type": "string"
        },
        "name": {
